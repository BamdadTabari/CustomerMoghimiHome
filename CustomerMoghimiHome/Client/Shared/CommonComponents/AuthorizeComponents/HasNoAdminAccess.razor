        <MudAlert Severity="Severity.Error" NoIcon="true" ContentAlignment="HorizontalAlignment.Center" >
            <MudIcon Class="m-5" Size="Size.Large"  Color="Color.Error" Icon="@Icons.Material.Filled.WavingHand"/>

            <MudDivider Class="m-5" Style="height:5px;color:black" />

            <MudText Class="m-5" Typo="Typo.h2" Align="Align.Center" Color="Color.Error">این صفحه مختص به ادمین سایت می باشد</MudText>

            <MudDivider Class="m-5" Style="height:5px;color:black"/>

            <MudText Class="m-5" Typo="Typo.h2" Align="Align.Center" Color="Color.Error">شما به این صفحه دسترسی ندارید</MudText>

            <MudDivider Class="m-5" Style="height:5px;color:black" />
        </MudAlert>
    </MudPaper>
</MudContainer>
